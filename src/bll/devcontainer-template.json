{
    "id": "bll",
    "version": "1.0.0",
    "name": "bll",
    "description": "Atari Lynx devcontainer template for BLL based development",
    "documentationURL": "https://github.com/AtariLynx/",
    "publisher": "Atari Lynx developers community",
    "licenseURL": "https://github.com/devcontainers/templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Alpine version:",
            "proposals": [
